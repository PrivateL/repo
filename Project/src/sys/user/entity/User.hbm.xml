<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="sys.user.entity.User" table="user">
		<id name="id">
			<column name="user_id"></column>
			<generator class="native"/>
		</id>
		<property name="username" column="user_name"/>
		<property name="password" column="user_password"/>
		<property name="email" column="user_email"/>
		<property name="phone" column="user_phone"/>
		<property name="state" column="user_state"/>
		<property name="code" column="user_code"/>
		<property name="create_date" column="user_create_time"/>
	</class>
</hibernate-mapping>